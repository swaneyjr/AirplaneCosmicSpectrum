#Macro file for example B1 test
#
/run/initialize
#
/gps/verbose 0
#
# configure the incident surface
/gps/pos/type Plane
/gps/pos/shape Square
/gps/pos/centre 0. 4. 0. m
/gps/pos/halfx 4. m
/gps/pos/halfy 4. m
/gps/pos/rot1 0. 0. -1.
/gps/pos/rot2 -1. 0. 0.
#
# proton spectrum 
/gps/particle proton
/gps/ang/type cos
/gps/ene/type Arb
/gps/hist/file spectrum.dat
/gps/hist/inter Exp
#

